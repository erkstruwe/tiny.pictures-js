{
  "name": "tiny.pictures-sdk-js",
  "version": "0.30.0",
  "description": "JavaScript SDK for tiny.pictures",
  "main": "dist/universal.js",
  "scripts": {
    "lint": "eslint .",
    "coverage": "istanbul cover --include-all-sources --captureExceptions --root src -x \"**/*.spec.js\" -x \"entry.js\" node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.json",
    "coverage:coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "unit": "node node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.json",
    "test": "npm run lint && npm run unit",
    "test:travis": "npm run lint && npm run coverage:coveralls",
    "build": "babel src/universal.js --out-file dist/universal.js && webpack",
    "preversion": "npm run build && npm test",
    "postversion": "git push && npm publish"
  },
  "engines": {
    "node": "^6.9.1",
    "npm": "^3.10.8"
  },
  "homepage": "https://github.com/erkstruwe/tiny.pictures-sdk-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erkstruwe/tiny.pictures-sdk-js.git"
  },
  "author": "Erk Struwe <estruwe@estruwe.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erkstruwe/tiny.pictures-sdk-js/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.10.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.2",
    "jsdom": "^9.8.3",
    "vanilla-lazyload": "^2.1.1",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "lodash": "^4.16.6",
    "urijs": "^1.18.2"
  }
}
