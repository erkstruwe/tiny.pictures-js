{
  "name": "tiny.pictures-sdk-js",
  "version": "0.39.0",
  "description": "JavaScript SDK for tiny.pictures",
  "main": "dist/universal.js",
  "scripts": {
    "lint": "eslint .",
    "unit": "jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "test": "npm run lint && npm run unit",
    "test:travis": "npm run test",
    "build": "babel src/universal.js --out-file dist/universal.js && webpack -p",
    "preversion": "npm run build && npm test",
    "version": "git add --all",
    "postversion": "git push origin master && npm publish"
  },
  "engines": {
    "node": "^6.9.5",
    "npm": "^3.10.10"
  },
  "homepage": "https://github.com/erkstruwe/tiny.pictures-sdk-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erkstruwe/tiny.pictures-sdk-js.git"
  },
  "author": "Erk Struwe <estruwe@estruwe.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erkstruwe/tiny.pictures-sdk-js/issues"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "sync-is-private-host": "^0.1.2",
    "urijs": "^1.18.7"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "coveralls": "^2.11.16",
    "eslint": "^3.15.0",
    "istanbul": "^0.4.5",
    "jasmine-es6": "^0.4.0",
    "jsdom": "^9.11.0",
    "vanilla-lazyload": "^4.0.4",
    "webpack": "^2.2.1"
  }
}
