{
  "name": "tiny.pictures-js",
  "version": "1.0.2",
  "description": "JavaScript library for tiny.pictures",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "unit": "jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "test": "npm run lint && npm run unit",
    "test:travis": "npm run test",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "babel src/index.js --out-file dist/index.js",
    "build:browser": "webpack -p",
    "watch:browser": "npm run build:browser -- --watch",
    "demo": "node demo",
    "start": "npm run demo",
    "preversion": "npm run build && npm test",
    "version": "git add --all",
    "postversion": "git push origin master && npm publish"
  },
  "engines": {
    "node": "^8.4.0",
    "npm": "^5.3.0"
  },
  "homepage": "https://github.com/erkstruwe/tiny.pictures-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erkstruwe/tiny.pictures-js.git"
  },
  "author": "Erk Struwe <estruwe@estruwe.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erkstruwe/tiny.pictures-js/issues"
  },
  "dependencies": {
    "lazysizes": "~4.0.0-rc3",
    "lodash": "^4.17.4",
    "sync-is-private-host": "^0.1.2",
    "urijs": "^1.18.7"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^4.6.1",
    "expose-loader": "^0.7.3",
    "express": "^4.14.1",
    "istanbul": "^0.4.5",
    "jasmine": "^2.8.0",
    "jsdom": "^11.2.0",
    "serve-index": "^1.8.0",
    "webpack": "^3.5.6",
    "winston": "^2.3.1"
  }
}
